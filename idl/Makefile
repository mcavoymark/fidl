runtime = /usr/local/bin/stats/runtime

checkout:
	co -q ${files_dat} ${functions} ${files} ${files_1} ${files_2} ${files_3} 
checkout64:
	co -q ${files_dat} ${functions} ${files} ${files_1} ${files_2} ${files_3} ${files_4}

checkout_lock:
	co -l -q ${files_dat} ${functions} ${files} ${files_1} ${files_2} ${files_3}


checkin:
	ci -f -q -r${REVISION} -msee_version.txt ${files_dat} ${functions} ${files}  ${files_1} ${files_2} ${files_3}

install:
	cd clib && /bin/rm *.o
	cd clib && make install
	make runtime
	chmod 0777 /usr/local/src/fidl/*
	cp ${functions} /usr/local/src/fidl
	cp ${files} /usr/local/src/fidl
	cp ${files_1} /usr/local/src/fidl
	cp ${files_2} /usr/local/src/fidl
	cp ${files_3} /usr/local/src/fidl
	chmod 0774 /usr/local/src/fidl/*

install_idl:
	make runtime
	cp ${functions} /usr/local/src/fidl
	cp ${files} /usr/local/src/fidl
	cp ${files_1} /usr/local/src/fidl
	cp ${files_2} /usr/local/src/fidl
	cp ${files_3} /usr/local/src/fidl

runtime:
	csh make_runtime.csh ${runtime}
	cp stats.h ${runtime}
	cp stats_init.h ${runtime}
	cp ECATdef.h ${runtime}
